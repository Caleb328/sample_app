

$('#event_star').raty({
  readOnly: true,
  path: '/assets',
  score: <%= @event.average_rate %>
});


$('#rate_event').raty({
  path: '/assets',
  score: <%= @rate.score %>,
  click: function(score, evt) {
    $.ajax({
      url: '/rates/' + <%= @rate.id %>,
      type: 'PATCH',
      data: { score: score }
    }).done(function (data){});
  }
});

$('#rate_score').html(<%= @rate.score %>); 